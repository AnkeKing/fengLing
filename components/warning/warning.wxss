/* components/warning/warning.wxss */
.warning-box{
    width: 100vw;
    height: 100vh;
    display: flex;
    justify-content: center;
    align-items: center;
    position: fixed;
    top: 0rpx;
    left: 0rpx;
    z-index: 99;
}
.warning{
    padding: 20rpx;
    border-radius: 5px;
    background: rgba(0, 0, 0,0.7);
    color: #ffffff;
    font-size: 30rpx;
}